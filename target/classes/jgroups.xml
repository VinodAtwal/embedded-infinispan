<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="urn:org:jgroups http://www.jgroups.org/schema/JGroups-5.0.xsd"
        xmlns="urn:org:jgroups">

    <TCP bind_addr="127.0.0.1" bind_port="7800" >
        <thread_pool min_threads="5" max_threads="100"/>
    </TCP>
    <TCPPING initial_hosts="127.0.0.1[7800]" port_range="3"/>
    <MERGE3/>
    <FD_SOCK/>
    <FD_ALL/>
    <VERIFY_SUSPECT/>
    <pbcast.NAKACK2 use_mcast_xmit="false"/>
    <UNICAST3/>
    <pbcast.STABLE/>
    <pbcast.GMS print_local_addr="true"/>
    <UFC/>
    <MFC/>
    <FRAG2/>
</config>
